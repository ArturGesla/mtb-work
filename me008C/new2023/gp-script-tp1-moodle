 "valeurs_instantanes.dat"  ev 1000 u 1:3 w lp lc -1 title "donnee2",\
 "valeurs_instantanes.dat"  ev 1000 u 1:4 w lp lc -1 title "donnee3",\
 "valeurs_instantanes.dat"  ev 1000 u 1:5 w lp lc -1 title "donnee4"\
  ;
  pause -1;
